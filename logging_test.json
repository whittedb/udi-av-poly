{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s:%(levelname)s:%(threadName)-9s:%(module)s.%(funcName)s - %(message)s",
      "datefmt": "%Y%m%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["console"]
  },
  "loggers": {
    "transitions": {
      "level": "INFO"
    }
  }
}
